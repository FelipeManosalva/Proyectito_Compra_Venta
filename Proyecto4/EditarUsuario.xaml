<Window x:Class="Proyecto4.EditarUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto4"
        mc:Ignorable="d"
        Title="Editar Usuario" Height="213.83" Width="359.574">
    <Grid>
        <Label x:Name="label" Content="Nombre Usuario:" HorizontalAlignment="Left" Margin="23,19,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Contraseña:" HorizontalAlignment="Left" Margin="23,69,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtnombreUsuario" HorizontalAlignment="Left" Height="23" Margin="151,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="172"/>
        <TextBox x:Name="txtContraseña" HorizontalAlignment="Left" Height="23" Margin="151,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="172"/>
        <Button x:Name="btnEditar" Content="Editar" HorizontalAlignment="Left" Margin="151,135,0,0" VerticalAlignment="Top" Width="75" Click="btnEditar_Click"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="248,135,0,0" VerticalAlignment="Top" Width="75" Click="btnLogin_Click"/>

    </Grid>
</Window>
